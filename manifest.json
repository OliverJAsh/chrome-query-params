{
    "name": "Query params",
    "description": "Easily manipulate URL query parameters.",
    "version": "0.0.8",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "mac": "Command+Shift+P"
            }
        }
    },
    "icons": {
        "128": "icon.png"
    }
}
