{
  "manifest_version": 3,
  "name": "Query params",
  "description": "Provides a user interface to read/write URL search/query parameters for the currently active tab.",
  "version": "0.0.9",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "mac": "Command+Shift+P",
        "windows": "Ctrl+Shift+P"
      }
    }
  }
}
